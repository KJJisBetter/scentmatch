{
  "name": "scentmatch",
  "version": "0.1.0",
  "description": "AI-powered fragrance discovery platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "eslint . --ext .ts,.tsx --max-warnings 5",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "quality": "npm run type-check && npm run lint:strict && npm run format:check",
    "quality:fix": "npm run type-check && npm run lint:fix && npm run format",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest run --reporter=verbose",
    "test:integration": "vitest run tests/integration",
    "test:a11y": "vitest run tests/accessibility --config=vitest.accessibility.config.ts",
    "test:a11y:browser": "npx playwright test --config=playwright.accessibility.config.ts",
    "test:a11y:full": "npm run test:a11y && npm run test:a11y:browser",
    "test:performance": "vitest run tests/performance",
    "test:home": "node scripts/qa/run-home-page-tests.js",
    "test:home:components": "vitest run tests/components/home-page.test.tsx",
    "test:home:a11y": "vitest run tests/accessibility/home-page-a11y.test.tsx",
    "test:home:performance": "vitest run tests/performance/home-page-performance.test.ts",
    "validate:home:performance": "node scripts/qa/validate-home-page-performance.js",
    "test:ci": "vitest run --coverage --reporter=junit --outputFile=test-results.xml",
    "test:coverage:threshold": "vitest --coverage --coverage.thresholds.lines=80 --coverage.thresholds.functions=75 --coverage.thresholds.branches=70",
    "lighthouse:mobile": "node tests/performance/lighthouse-mobile.js",
    "validate:setup": "vitest run tests/setup-validation",
    "validate:build": "node scripts/qa/run-build-validation.js",
    "validate:build:critical": "npm run test tests/setup-validation/technology-compatibility.test.ts && npm run test tests/setup-validation/build.test.ts",
    "validate:supabase": "tsx scripts/validate-supabase.ts",
    "validate:data": "tsx scripts/database/validate-data.ts",
    "import:brands": "tsx scripts/database/import-brands.ts",
    "import:fragrances": "tsx scripts/database/import-fragrances.ts",
    "import:full": "tsx scripts/database/execute-full-import.ts",
    "extract:kaggle": "tsx scripts/kaggle-rebuild/extract-top-2000-fragrances.ts",
    "test:import": "tsx scripts/database/test-import.ts",
    "test:verify": "node scripts/testing/run-critical-verification.js",
    "test:verify:critical": "npx playwright test tests/verification/critical-platform-verification.test.ts --reporter=list",
    "test:verify:summary": "node scripts/testing/verification-summary.js",
    "test:frontend:verify": "node scripts/qa/run-frontend-verification.js",
    "test:e2e:full": "npx playwright test tests/qa/end-to-end-verification.test.ts --config=playwright.e2e.config.ts",
    "audit:fragrance-data": "tsx scripts/database/audit-fragrance-data.ts",
    "fix:fragrance-families": "tsx scripts/database/fix-fragrance-families.ts",
    "fix:fragrance-families:live": "tsx scripts/database/fix-fragrance-families.ts --live",
    "test:data-quality": "vitest run tests/database/fragrance-data-consistency.test.ts tests/lib/data-validation/fragrance-data-validator.test.ts",
    "test:cross-device": "npx playwright test --config=playwright.cross-device.config.ts",
    "test:cross-device:core": "npx playwright test --config=playwright.cross-device.config.ts tests/cross-device/core-user-flows.test.ts",
    "test:cross-device:a11y": "npx playwright test --config=playwright.cross-device.config.ts tests/cross-device/accessibility-cross-device.test.ts",
    "test:cross-device:perf": "npx playwright test --config=playwright.cross-device.config.ts tests/cross-device/performance-analysis.test.ts",
    "test:cross-device:network": "npx playwright test --config=playwright.cross-device.config.ts tests/cross-device/network-throttling.test.ts",
    "analyze:bundle": "node scripts/performance/bundle-analyzer.js",
    "test:cross-device:full": "node scripts/qa/run-cross-device-tests.js",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings 5",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix --max-warnings 5",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.17",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.45.0",
    "@tanstack/react-table": "^8.21.3",
    "@upstash/ratelimit": "^2.0.6",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "ai": "^5.0.19",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^1.1.1",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.1",
    "fuse.js": "^7.1.0",
    "lucide-react": "^0.454.0",
    "mixpanel-browser": "^2.69.1",
    "nanoid": "^5.1.5",
    "next": "^15.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^2.0.0",
    "voyageai": "^0.0.8",
    "web-vitals": "^5.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.0",
    "@axe-core/react": "^4.10.2",
    "@next/bundle-analyzer": "^15.5.0",
    "@playwright/test": "^1.54.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.8.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^15.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^25.0.0",
    "lighthouse": "^12.8.1",
    "lint-staged": "^16.1.5",
    "playwright": "^1.40.0",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "fragrance",
    "ai",
    "recommendations",
    "nextjs",
    "supabase"
  ],
  "author": "ScentMatch Team",
  "license": "MIT"
}
